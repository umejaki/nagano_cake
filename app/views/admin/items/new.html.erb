<p>商品新規登録</p>
<table>
  <tbody>
    <%= form_with model: @item do |f| %>
    <tr>
      <td>商品情報</td>
      <td><%= f.file_field :image, accept: "image/*" %></td>
    </tr>
    <tr>
      <td>商品名</td>
      <td><%= f.file_field :title %></td>
    </tr>
    <tr>
      <td>商品説明</td>
      <td><%= f.text_area :body %></td>
    </tr>
    <tr>
      <td>商品価格</td>
      <td></td>
    </tr>
  </tbody>
</table>